  "version": "0.2.0",
  "configurations": [
    {
      "name": "Upload Plot Images",
      "type": "node",
      "request": "launch",
      "args": ["${workspaceRoot}/scripts/uploadExistingPlotImages.ts", "https://ipfs.infura.io/ipfs", "https://mainnet.infura.io:443/lupI6IMUAMmSNzzX1TJT", "0x5c0bd5d00588a4f004ce0d92958c96264026f7be", "eth-plot-data", "mainnet"],
      // "args": ["${workspaceRoot}/scripts/uploadExistingPlotImages.ts", "https://ipfs.infura.io/ipfs", "https://ropsten.infura.io:443/lupI6IMUAMmSNzzX1TJT", "0x8f87224ce1631e7f20503cd510538a7d2f4d7977", "eth-plot-data", "ropsten"],
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
      "sourceMaps": true,
      "cwd": "${workspaceRoot}",
      "protocol": "inspector",
    },
    {
        "type": "node",
        "request": "launch",
        "name": "truffle test (debugable)",
        "cwd": "${workspaceFolder}",
        "program": "${workspaceFolder}/node_modules/truffle-core/cli.js",
        "args": [
            "test",
            "--network",
            "ganache"
        ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run Current File",
      "program": "${file}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/devServer.js"
    }
  ]
}